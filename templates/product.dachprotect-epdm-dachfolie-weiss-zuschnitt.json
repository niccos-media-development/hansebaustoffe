            "custom_liquid": "{% if product.selected_or_first_available_variant.available %}\n<div class=\"lb-delivery-time-holder\">\n  <div class=\"lb-delivery-time-3\">\n    <div class=\"delivery-wrapper\">\n      <div class=\"delivery-content\">\n        <div class=\"delivery-icon\">\n          <svg width=\"24px\" height=\"24px\" viewBox=\"0 0 50 29\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\">\n            <g stroke=\"#54844C\" stroke-width=\"3\" stroke-linecap=\"round\" stroke-linejoin=\"round\" fill=\"none\">\n              <line x1=\"5.25\" y1=\"12\" x2=\"13.75\" y2=\"12\"></line>\n              <line x1=\"0.25\" y1=\"16\" x2=\"8.75\" y2=\"16\"></line>\n              <line x1=\"5.23\" y1=\"22\" x2=\"10.77\" y2=\"22\"></line>\n              <polyline points=\"8 9.66 8 0 31 0 31 22 22.8 22\"></polyline>\n              <polyline points=\"31 4 41.35 4 46 11.81 46 20 44.08 20\"></polyline>\n              <circle cx=\"15.5\" cy=\"21.5\" r=\"3.5\"></circle>\n              <circle cx=\"36.5\" cy=\"21.5\" r=\"3.5\"></circle>\n            </g>\n          </svg>\n        </div>\n        <span class=\"delivery-info\">In 2-5 Tagen geliefert</span>\n      </div>\n      <span class=\"shipping-text\">\n        <a href=\"#\" class=\"shipping-link\" onclick=\"handleShippingClick(event)\">Versand</a> wird im Checkout berechnet\n      </span>\n    </div>\n  </div>\n</div>\n\n<div id=\"popup\" class=\"popup-overlay\" onclick=\"closePopupOutside(event)\">\n  <div class=\"popup-content\">\n    <span class=\"close-btn\" onclick=\"closePopup()\">&times;</span>\n    <h3>Versand- und Zahlungsbedingungen</h3>\n    <p>Hier finden Sie alle wichtigen Informationen zu Versandkosten, Lieferzeiten und Zahlungsmöglichkeiten.</p>\n    <button class=\"popup-button\" onclick=\"redirectToShipping()\">Mehr erfahren</button>\n  </div>\n</div>\n\n<style>\n.lb-delivery-time-holder {\n  margin: -35px 0px -35px 0px!important;\n  display: block!important;\n}\n\n.lb-delivery-time-3 {\n  display: flex;\n  align-items: center;\n  font-size: 12px!important;\n  line-height: 20px!important;\n  flex-wrap: wrap;\n}\n\n.delivery-wrapper {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n  flex-wrap: wrap;\n}\n\n.delivery-content {\n  display: flex;\n  align-items: center;\n  background: #BFF1B7;\n  padding: 8px 15px;\n  border-radius: 50px;\n  flex-shrink: 0;\n  white-space: nowrap;\n  gap: 10px;\n}\n\n.delivery-icon {\n  display: flex;\n  align-items: center;\n  flex-shrink: 0;\n}\n\n.lb-delivery-time-3 svg g {\n  stroke: #54844C!important;\n}\n\n.delivery-info {\n  color: #54844C!important;\n  font-size: 14px!important;\n  font-weight: bold;\n  white-space: nowrap;\n}\n\n.shipping-text {\n  font-size: 12px;\n  color: #000000;\n  white-space: nowrap;\n  flex-grow: 1;\n}\n\n.shipping-link {\n  text-decoration: underline !important;\n  color: #000000 !important;\n  cursor: pointer;\n}\n\n.popup-overlay {\n  display: none;\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.5);\n  justify-content: center;\n  align-items: center;\n  z-index: 1000;\n}\n\n.popup-content {\n  background: #fff;\n  padding: 20px;\n  border-radius: 10px;\n  text-align: center;\n  width: 90%;\n  max-width: 400px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.2);\n  position: relative;\n}\n\n.popup-button {\n  background: #54844C;\n  color: #fff;\n  padding: 10px 15px;\n  border: none;\n  border-radius: 5px;\n  font-size: 14px;\n  cursor: pointer;\n  margin-top: 10px;\n}\n\n.popup-button:hover {\n  background: #3A5E32;\n}\n\n.close-btn {\n  position: absolute;\n  top: 10px;\n  right: 15px;\n  font-size: 20px;\n  cursor: pointer;\n}\n\n@media (max-width: 768px) {\n  .lb-delivery-time-3 {\n    justify-content: flex-start;\n  }\n\n  .delivery-wrapper {\n    flex-wrap: wrap;\n  }\n\n  .delivery-content {\n    padding: 6px 10px;\n    gap: 8px;\n  }\n\n  .shipping-text {\n    font-size: 11px;\n  }\n\n  .popup-content {\n    width: 90%;\n  }\n}\n</style>\n\n<script>\n  function handleShippingClick(event) {\n    event.preventDefault();\n    \n    if (window.innerWidth <= 768) {\n      document.getElementById(\"popup\").style.display = \"flex\";\n    } else {\n      window.location.href = \"https://www.hanse-syntec.de/pages/versand-und-zahlungsbedingungen\";\n    }\n  }\n\n  function redirectToShipping() {\n    window.location.href = \"https://www.hanse-syntec.de/pages/versand-und-zahlungsbedingungen\";\n  }\n\n  function closePopup() {\n    document.getElementById(\"popup\").style.display = \"none\";\n  }\n\n  function closePopupOutside(event) {\n    if (event.target.classList.contains(\"popup-overlay\")) {\n      closePopup();\n    }\n  }\n</script>\n{% endif %}",

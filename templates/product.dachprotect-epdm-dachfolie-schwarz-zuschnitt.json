mportant;\n}\n\n.stock-info {\n  background: #BFF1B7;\n  font-size: 14px!important;\n  padding: 3px 18px;\n  border-radius: 50px;\n  display: inline-flex;\n  align-items: center;\n  font-weight: bold;\n  margin-right: 10px;\n}\n\n.stock-info svg {\n  width: 20px;\n  height: 20px;\n  margin-right: 9px;\n}\n\n.delivery-info {\n  font-weight: bold;\n}\n\n.shipping-text {\n  font-size: 12px;\n  color: #000000;\n  display: block;\n  margin-top: 5px;\n}\n\n.shipping-link {\n  text-decoration: underline !important;\n  color: #000000 !important;\n  cursor: pointer;\n}\n\n/* Popup Styling */\n.popup-overlay {\n  display: none;\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.5);\n  justify-content: center;\n  align-items: center;\n  z-index: 1000;\n}\n\n.popup-content {\n  background: #fff;\n  padding: 20px;\n  border-radius: 10px;\n  text-align: center;\n  width: 90%;\n  max-width: 400px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.2);\n  position: relative;\n}\n\n.popup-button {\n  background: #54844C;\n  color: #fff;\n  padding: 10px 15px;\n  border: none;\n  border-radius: 5px;\n  font-size: 14px;\n  cursor: pointer;\n  margin-top: 10px;\n}\n\n.popup-button:hover {\n  background: #3A5E32;\n}\n\n.close-btn {\n  position: absolute;\n  top: 10px;\n  right: 15px;\n  font-size: 20px;\n  cursor: pointer;\n}\n\n@media (max-width: 768px) {\n  .lb-delivery-time-3 {\n    flex-direction: column;\n    align-items: flex-start;\n  }\n\n  .shipping-text {\n    font-size: 11px;\n  }\n\n  .popup-content {\n    width: 90%;\n  }\n}\n</style>\n\n<script>\n  function handleShippingClick(event) {\n    event.preventDefault();\n    \n    if (window.innerWidth <= 768) {\n      document.getElementById(\"popup\").style.display = \"flex\";\n    } else {\n      window.location.href = \"https://www.hanse-syntec.de/pages/versand-und-zahlungsbedingungen\";\n    }\n  }\n\n  function redirectToShipping() {\n    window.location.href = \"https://www
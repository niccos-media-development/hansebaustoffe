{% if product.metafields.custom.configurable_product_type.value != blank %}
  <script>
    const form = document;
    const originalAddEventListener = form.addEventListener;
    const formEventListeners = [];
    form.addEventListener = function (type, listener, options) {
      if(type === 'submit') return;
      //if(!listener.toString().startsWith('onSubmitHandler(event){')) return;
      formEventListeners.push({ type, listener });
      originalAddEventListener.call(form, type, listener, options);
    };

    function removeFormListener(type, listener) {
      const listenerToRemove = formEventListeners.find(
        (entry) => entry.type === type && entry.listener === listener
      );
    
      if (listenerToRemove) {
        form.removeEventListener(type, listenerToRemove.listener);
      }
    }

    const formatter = new Intl.NumberFormat(Shopify.locale, { style: "currency", currency: Shopify.currency.active });
    function update10064() {
      let [ p_dicke, p_breite, p_launge ] = [ "Materialstärke", "Breite", "Länge" ].map(f =>
        document.getElementById('LineItemProperty-' + f)?.value
      ).map(n => n ? Number(n.match(/^[0-9,.]+/)?.[0]?.replaceAll(",", ".")) : null);

      let sku,
          dicke_fmt = Math.floor(p_dicke * 10),
          breite_fmt = Math.floor(p_breite * 100).toString().padStart(3, '0').slice(0,3),
          launge_fmt = Math.floor(p_launge * 10).toString().padStart(3, '0').slice(0,3);

      {% assign base_prices = product.metafields.custom.base_price_list | newline_to_br | split: "<br />" %}
      let basePrice, basePrices = {{ base_prices | json }};
      basePrices = basePrices.map(basePrice => {
        basePrice = basePrice.replace('\n', '');
        let [ measurement, price ] = basePrice.split(' / ');
        return [Number(measurement.match(/^[0-9,.]+/)?.[0]?.replaceAll(",", ".")), Number(price.match(/^[0-9,.]+/)?.[0]?.replaceAll(",", "."))]
      })

      {% case product.metafields.custom.configurable_product_type.value %}
        {% when 'dachfolie-schwarz' -%}
          sku = `D${dicke_fmt}-${breite_fmt}-${launge_fmt}`;
          basePrices.forEach(([measurement, price]) => {
            if(p_dicke === measurement)
              basePrice = price
          })
        {% when 'dachfolie-weiss' -%}
          sku = `DWS-${breite_fmt}-${launge_fmt}`;
          basePrices.forEach(([measurement, price]) => {
            if(p_dicke === measurement)
              basePrice = price
          })
        {% when 'abdichtungsplane' -%}
          sku = `B${dicke_fmt}-${breite_fmt}-${launge_fmt}`;
          basePrices.forEach(([measurement, price]) => {
            if(p_dicke === measurement)
              basePrice = price
          })
        {% when 'form-nahtabdeckband' %}
          sku = `FAB-S${breite_fmt}-${Math.floor(p_launge * 100).toString().padStart(3, '0').slice(0,3)}`;
          basePrices.forEach(([measurement, price]) => {
            if(p_breite === measurement)
              basePrice = price
          })
      {%- endcase %}

      
      let price = Math.round(basePrice * {% if product.metafields.custom.configurable_product_type.value != "form-nahtabdeckband" %} p_breite *{% endif %} p_launge * 1000) / 1000;

      document.querySelector('[data-price]').innerText = formatter.format(price);
      document.querySelector('[data-current-base-price]').innerText = formatter.format(basePrice);
      document.querySelector('[data-current-quadratmeter]').innerText = {% if product.metafields.custom.configurable_product_type.value == "form-nahtabdeckband" %}Number((p_launge).toFixed(2)).toString();{% else %}Number((p_breite * p_launge).toFixed(2)).toString();{% endif %}
      document.querySelector('[data-current-sku]').innerText = sku;
    }
    
    document.querySelectorAll('[id^=LineItemProperty-]').forEach(select => {
      select.addEventListener('change', update10064)
    })

    update10064();
  </script>
{% endif %}


{% comment %}theme-check-disable TemplateLength{% endcomment %}
{{ 'section-main-product.css' | asset_url | stylesheet_tag }}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-facets.css' | asset_url | stylesheet_tag }}
{{ 'component-rating.css' | asset_url | stylesheet_tag }}
{{ 'component-pills.css' | asset_url | stylesheet_tag }}
{{ 'component-tooltip.css' | asset_url | stylesheet_tag }}
{{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
{% if product.media.size > 0 %}
	{{ 'flickity.css' | asset_url | stylesheet_tag }}
{% endif %}


<style>
  .carousel-slider product-card .card-footer {
    height : auto;
  }

  .carousel-slider product-card .card-info {
    flex : 1;
  }
</style>

<link rel="stylesheet" href="{{ 'component-reviews.css' | asset_url }}" media="print" onload="this.media='all'">

<noscript>{{ 'component-reviews.css' | asset_url | stylesheet_tag }}</noscript>

{% liquid
	assign first_3d_model = product.media | where: "media_type", "model" | first
	assign product_form_id = 'product-form-' | append: section.id
	assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'

	assign is_pre_order = false
	if product.template_suffix == 'pre-order'
		assign is_pre_order = true
	endif
%}

{%- if first_3d_model -%}
	<link id="ModelViewerStyle" rel="stylesheet" href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css" media="print" onload="this.media='all'">
	<link id="ModelViewerOverride" rel="stylesheet" href="{{ 'component-model-viewer-ui.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

<div class="container {% if product.media.size == 0 %}container-narrow{% endif %}">
	<div class="block-section">
		<div class="product product-page {% if product.media.size == 0 %}product-no-media{% endif %}">
			{% if product.media.size > 0 %}
        <div class="hs-product-gallery-container">
          <div class="product-media-gallery-wrap">
            <a class="skip-link visually-hidden visually-hidden-focusable" href="#Product-{{ section.id }}">{{ 'accessibility.link_messages.skip_to_product_info' | t }}</a>
            <media-gallery id="MediaGallery-{{ section.id }}" role="region" class="product-media-gallery" aria-label="{{ 'products.product.media.gallery_viewer' | t }}" hide-variants="{{ section.settings.hide_variants }}">
              <div class="product-media-wrapper">
                {% if product.media %}
                  {%- assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media -%}

                  <div class="product-media-main-wrapper" data-aos="zoom-in">
                    {%- assign media_position = 1 -%}
                    <div class="product-media-main product-media is-active" data-media-id="{{ featured_media.id }}">
                      {% render 'product-gallery-media',
                        media: featured_media,
                        position: media_position,
                        media_width: 800,
                        lazyload: false,
                        modal_id: section.id,
                        image_zoom_type: section.settings.image_zoom_type
                      %}
                    </div>

                    {%- for media in product.media -%}
                      {%- if media.id != featured_media.id -%}
                        {%- liquid
                          assign media_position = media_position | default: 1 | plus: 1
                          assign variant_image = false
                          if section.settings.hide_variants and variant_images contains media.src
                            assign variant_image = true
                          endif
                        -%}

                        {% unless variant_image %}
                          <div class="product-media-main product-media" data-media-id="{{ media.id }}">
                            {% render 'product-gallery-media',
                              media: media,
                              position: media_position,
                              media_width: 800,
                              lazyload: true,
                              modal_id: section.id,
                              image_zoom_type: section.settings.image_zoom_type
                            %}
                          </div>
                        {% endunless %}
                      {%- endif -%}
                    {%- endfor -%}
                  </div>

                  {%- assign media_position = 1 -%}
                  <div class="product-media-scroll-wrap">
                    <div class="product-media-scroller">
                      <div class="product-media-thumbnails">
                        <div data-aos="fade-up" class="product-media-thumbnail product-media is-active
                            {% if section.settings.hide_variants and variant_images contains featured_media.src %} product-media-thumbnail-variant{% endif %}"
                          data-media-id="{{ featured_media.id }}">
                          {% render 'product-gallery-media-thumbnail',
                            media: featured_media, position: media_position, media_width: 400, lazyload: false
                          %}
                        </div>

                        {% for media in product.media %}
                          {%- if media.id != featured_media.id -%}
                            {%- assign media_position = media_position | default: 1 | plus: 1 -%}
                            <div data-aos="fade-up" data-aos-delay="{{ 100 | times: forloop.index }}" class="product-media-thumbnail product-media
                                {% if section.settings.hide_variants and variant_images contains media.src %} product-media-thumbnail-variant{% endif %}"
                              data-media-id="{{ media.id }}">
                              {% render 'product-gallery-media-thumbnail',
                                media: media, position: media_position, media_width: 400, lazyload: true
                              %}
                            </div>
                          {% endif %}
                        {% endfor %}
                      </div>
                    </div>

                    {% if section.settings.thumbnail_navigation %}
                      <product-thumbnail-navigation for="MediaGallery-{{ section.id }}" class="hidden">
                        <div class="pill-nav pill-nav-slider media-pill-nav-slider">
                          <button class="pill-nav-item pill-nav-item-prev pill-nav-item-disabled focus-none js-prev color-background-1" aria-disabled="true">
                            <span class="visually-hidden">{{ 'sections.slideshow.previous_slide' | t }}</span>
                            {% render 'angle', direction: 'up' %}
                          </button>
                          <button class="pill-nav-item pill-nav-item-next js-next color-background-1" aria-disabled="false">
                            <span class="visually-hidden">{{ 'sections.slideshow.next_slide' | t }}</span>
                            {% render 'angle', direction: 'down' %}
                          </button>
                        </div>
                      </product-thumbnail-navigation>
                    {% endif %}
                  </div>
                {% endif %}
              </div>
            </media-gallery>

            {% if section.settings.image_zoom_type != 'none' %}
              <product-modal id="ProductModal-{{ section.id }}" class="product-media-modal media-modal">
                <div
                  class="product-media-modal-dialog"
                  role="dialog"
                  aria-modal="true"
                  aria-label="{{ 'products.product.media.gallery' | t }}"
                  tabindex="-1"
                >
                  <button id="ModalClose-{{ section.id }}" type="button" class="product-media-modal-toggle" aria-label="{{ 'accessibility.close' | t }}">
                    {% render 'icon-cross' %}
                  </button>
                  <div class="product-media-modal-content" role="document" aria-label="{{ 'products.product.media.gallery' | t }}" tabindex="0">
                    {%- liquid
                      if product.selected_or_first_available_variant.featured_media != null
                        assign media = product.selected_or_first_available_variant.featured_media
                        render 'product-gallery-media-modal', media: media, variant_image: section.settings.hide_variants
                      endif
                    -%}

                    {%- for media in product.media -%}
                      {%- liquid
                        assign variant_image = false
                        if section.settings.hide_variants and variant_images contains media.src
                          assign variant_image = true
                        endif

                        unless media.id == product.selected_or_first_available_variant.featured_media.id or variant_image
                          render 'product-gallery-media-modal', media: media, variant_image: variant_image
                        endunless
                      -%}
                    {%- endfor -%}
                  </div>
                </div>
              </product-modal>
            {% endif %}

            {% if section.settings.image_disclaimer != blank %}
              <div class="product-media-gallery-message">
                <div class="rte">
                  {{ section.settings.image_disclaimer }}
                </div>
              </div>
            {% endif %}
          </div>
          <div class="product-info-description main_new mobile--hidden">
            <div class="product-info-description-content">
              {% for block in section.blocks %}
                {% case block.type %}
                  {%- when 'custom_liquid' -%}
                    {% liquid
                      if block.settings.position != "left"
                        continue
                      endif
                    %}
                    {% capture liquid_output %}{{ block.settings.custom_liquid }}{% endcapture %}
                    {% if liquid_output != blank %}
                      {% if block.settings.title != blank or block.settings.icon != blank %}
                        {% render 'niccos-collapse', 
                          content: block.settings.custom_liquid,
                          block: block,
                          attributes: block.shopify_attributes
                        %}
                      {% else %}
                        <div class="facet facet-standalone" {{ block.shopify_attributes }}>
                          <div class="facet-content rte">
                            {{ block.settings.custom_liquid }}
                          </div>
                        </div>
                      {% endif %}
                    {% endif %}
    
                  {% when 'description' %}
                    {% if product.description != blank %}
                      {% render 'niccos-collapse', 
                        content: product.description,
                        block: block
                      %}
                    {% endif %}
    
                  {% when 'reviews' %}
                    {% render 'niccos-collapse', 
                      content: 'product-review-widget',
                      block: block,
                      id: 'product-reviews',
                      class: 'product-reviews',
                      is_widget: true
                    %}
    
                    {% when 'download' %}
      
                      {% render 'niccos-meta1', 
              
                        block: block
                      %}
                    {% when 'videometa' %}
      
                      {% render 'niccos-meta2', 
              
                        block: block
                      %}                    
    
                  {% when 'expandable' %}
                    {% if block.settings.text != blank %}
                      {% render 'niccos-collapse', 
                        content: block.settings.text,
                        block: block
                      %}
                    {% endif %}
                {% endcase %}
              {% endfor %}
            </div>
          </div>
        </div>
			{% endif %}

			<div class="product-info-wrap-content">
				<div id="Product-{{ section.id }}" class="product-info">
					{% for block in section.blocks %}
						{% case block.type %}
							{% when '@app' %}
								<div class="product-info-secondary" {{ block.shopify_attributes }}>
									{% render block %}
								</div>

							{%- when 'text' -%}
								{% if block.settings.text != blank or block.settings.text_2 != blank or block.settings.text_3 != blank or block.settings.text_4 != blank %}
									{% if block.settings.color_scheme != "none" %}
										<div class="product-info-secondary card color-{{ block.settings.color_scheme }}" {{ block.shopify_attributes }}>
									{% else %}
										<div class="product-info-secondary" {{ block.shopify_attributes }}>
									{% endif %}
										{% if block.settings.text != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon != blank %}
													{% render 'icons', icon: block.settings.icon %}
												{% endif %}
												{{ block.settings.text }}
											</div>
										{% endif %}
										{% if block.settings.text_2 != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon_2 != blank %}
													{% render 'icons', icon: block.settings.icon_2 %}
												{% endif %}
												{{ block.settings.text_2 }}
											</div>
										{% endif %}
										{% if block.settings.text_3 != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon_3 != blank %}
													{% render 'icons', icon: block.settings.icon_3 %}
												{% endif %}
												{{ block.settings.text_3 }}
											</div>
										{% endif %}
										{% if block.settings.text_4 != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon_4 != blank %}
													{% render 'icons', icon: block.settings.icon_4 %}
												{% endif %}
												{{ block.settings.text_4 }}
											</div>
										{% endif %}
									</div>
								{% endif %}

							{% when 'title' %}
								<div class="product-info-main" {{ block.shopify_attributes }}>
									{% if block.settings.show_custom_badges %}
										{% render 'card-product-custom-badges', product: product, class: 'product-info-badges' %}
									{% endif %}

									{% if block.settings.show_vendor and product.vendor != blank %}
										{% liquid
											assign matching_collection = false
											assign vendor_handle = product.vendor | handleize
											for collection in product.collections
												if collection.handle == vendor_handle
													assign matching_collection = true
												endif
											endfor
										%}
										<div class="product-info-eyebrow">
											{% if matching_collection %}
												<a href="{{ routes.collections_url }}/{{ vendor_handle }}">{{ product.vendor }}</a>
											{% else %}
												{{ product.vendor | link_to_vendor }}
											{% endif %}
										</div>
									{% endif %}

                  <h1 
                     data-aos="fade-down" data-aos-duration="500"
                    class="product-info-heading h2">
										{{ product.title | escape }}
									</h1>

									{% liquid
										assign product_info_meta_stretch_class = ''
										if product.selected_or_first_available_variant.barcode and block.settings.show_barcode and product.selected_or_first_available_variant.sku and block.settings.show_sku
											assign product_info_meta_stretch_class = 'product-info-meta-stretch'
										endif
									%}

									{% if block.settings.show_stock or block.settings.show_sku or block.settings.show_barcode or block.settings.show_rating %}
                    <div 
                      data-aos="fade-right"
                      id="ProductInfoMeta-{{ section.id }}" class="product-info-meta {{ product_info_meta_stretch_class }} {% if product.selected_or_first_available_variant == nil %}visually-hidden{% endif %}">
											{% unless settings.catalog_enabled %}
												{% if block.settings.show_stock %}
													<div id="ProductAvailability-{{ section.id }}" class="product-info-meta-item">
														{% render 'product-availability', product: product %}
													</div>
												{% endif %}
											{% endunless %}

											{% if block.settings.show_sku %}
												{% if product.selected_or_first_available_variant.sku != blank %}
													<span class="product-info-meta-item product-info-meta-item-code">
														{{ 'products.product.sku' | t }}: <span id="ProductSku-{{ section.id }}" {%- if product.metafields.custom.configurable_product_type.value != blank %} data-current-sku{% endif %}>{{ product.selected_or_first_available_variant.sku }}</span>
													</span>

                                
												{% endif %}
											{% endif %}

											{% if block.settings.show_barcode %}
												{% if product.selected_or_first_available_variant.barcode != blank %}
													<span class="product-info-meta-item product-info-meta-item-code">
														{% if block.settings.barcode_label != blank %}
															{{ block.settings.barcode_label }}:
														{% endif %}
														<span id="ProductBarcode-{{ section.id }}">{{ product.selected_or_first_available_variant.barcode }}</span>
													</span>
												{% endif %}
											{% endif %}

											{% if block.settings.show_rating %}
												<div class="product-info-meta-item last {% if block.settings.hide_empty_star_rating %}product-rating-hide-empty{% endif %}">
													{% render 'product-rating-widget', product: product %}
												</div>
											{% endif %}
										</div>
									{% endif %}
								</div>
							{% when 'extra' %}
								<div class="product-info-extra" {{ block.shopify_attributes }}>
									{% if block.settings.text != blank %}
										<div class="product-info-extra-aux rte">
											{{ block.settings.text }}
										</div>
									{% endif %}

									{% if block.settings.show_facebook or block.settings.show_twitter or block.settings.show_linkedin %}
										<div class="product-info-extra-sharing">
											<ul class="list-social">
												{% if block.settings.show_facebook %}
													<li class="list-social-item">
														<a href="https://www.facebook.com/sharer.php?u={{ shop.url }}{{ product.url | url_encode }}" data-share-url="https://www.facebook.com/sharer.php?u=" target="_blank" class="list-social-link">
															<span class="visually-hidden">{{ 'general.social.share_on_facebook' | t }}</span>
															{% render 'icon-facebook' %}
														</a>
													</li>
												{% endif %}
												{% if block.settings.show_twitter %}
													<li class="list-social-item">
														<a href="https://twitter.com/share?url={{ shop.url }}{{ product.url | url_encode }}" data-share-url="https://twitter.com/share?url=" target="_blank" class="list-social-link">
															<span class="visually-hidden">
																{% if settings.social_link_twitter_use_x %}
																	{{ 'general.social.links.x' | t }}
																{% else %}
																	{{ 'general.social.links.twitter' | t }}
																{% endif %}
															</span>
															{% if settings.social_link_twitter_use_x %}
																{% render 'icon-x' %}
															{% else %}
																{% render 'icon-twitter' %}
															{% endif %}
														</a>
													</li>
												{% endif %}
												{% if block.settings.show_linkedin %}
													<li class="list-social-item">
														<a href="https://www.linkedin.com/sharing/share-offsite/?url={{ shop.url }}{{ product.url | url_encode }}" data-share-url="https://www.linkedin.com/sharing/share-offsite/?url=" target="_blank" class="list-social-link">
															<span class="visually-hidden">{{ 'general.social.share_on_linkedin' | t }}</span>
															{% render 'icon-linkedin' %}
														</a>
													</li>
												{% endif %}
											</ul>
										</div>
									{% endif %}
								</div>

							{%- when 'price' -%}
								{% unless settings.catalog_enabled %}
                  <div 
                      data-aos="fade-right"
                    class="{% unless product.has_only_default_variant %}no-js-hidden{% endunless %} {% if product.selected_or_first_available_variant == nil %}visually-hidden{% endif %}" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>

                    {% if product.metafields.custom.configurable_product_type.value != blank %}
                      {%- assign show_base_price = true -%}
                    {% endif %}

										{% render 'price', show_base_price: show_base_price, product: product, discount_badge: true, use_variant: true %}
									</div>

									{%- form 'product', product, id: 'product-form-installment', class: 'installment', data-event-context: 'MainProductPageInstallment' -%}
										<input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
										{{ form | payment_terms }}
									{%- endform -%}
								{% endunless %}

                            {%- when 'line_item_property' -%}
                                {% render 'line-item-property',
									section: section,
									block: block,
									product_form_id: product_form_id
								%}
                          
							{%- when 'variants' -%}
								{% render 'product-variant-picker',
									section: section,
									block: block,
									product: product,
									product_form_id: product_form_id,
									update_url: true
								%}

							{% when 'buy_buttons' %}
                              {% capture buy_buttonz %}
								{% unless settings.catalog_enabled %}
									{%- liquid
										assign gift_card_recipient_feature_active = false
										if block.settings.show_gift_card_recipient and product.gift_card?
											assign gift_card_recipient_feature_active = true
										endif

										assign show_dynamic_checkout = false
										if block.settings.show_buy_now and gift_card_recipient_feature_active == false and is_pre_order == false
											assign show_dynamic_checkout = true
										endif
									-%}

									<product-form class="product-form" data-hide-errors="{{ gift_card_recipient_feature_active }}" {{ block.shopify_attributes }}>
										<div class="product-form-error-message-wrapper form-status form-status-error" role="alert" hidden>
											<span class="product-form-error-message"></span>
										</div>

										{% form 'product', product, id: product_form_id, class: 'form', novalidate: 'novalidate', data-type: 'add-to-cart-form', data-stock-threshold: settings.only_x_left, data-event-context: 'MainProductPage' %}
											{% if product.metafields.custom.configurable_product_type.value != blank %}
                                              <input type="hidden" name="variant-search-collection" value="{{ product.metafields.custom.configurable_product_type.value }}" />
                                            {% endif %}
                                                <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}"
												{% if product.selected_or_first_available_variant.available == false
													or product.selected_or_first_available_variant == nil
												%}
													disabled
												{% endif %}
											>

											{%- if gift_card_recipient_feature_active -%}
												{%- render 'gift-card-recipient-form', product: product, form: form, section: section -%}
											{%- endif -%}
                                            {% if block.settings.show_quantity %}
                                              <quantity-input 
                                                data-aos="fade-right"
                                                class="quantity-input">
                                                    <button type="button" name="minus" class="quantity-input-button quantity-input-minus no-js-hidden">
                                                        &minus;
                                                        <span class="visually-hidden">{{ 'products.product.quantity.decrease' | t: product: product.title | escape }}</span>
                                                    </button>
                                                    <label for="quantity-input-{{ section.id }}" class="visually-hidden">{{ 'products.product.quantity.label' | t }}</label>
                                                    <input name="quantity"
                                                        type="number"
                                                        class="quantity-input-field"
                                                        id="quantity-input-{{ section.id }}"
                                                        min="1"
                                                        value="1"
                                                        form="{{ product_form_id }}"
                                                    >
                                                    <button type="button" name="plus" class="quantity-input-button quantity-input-plus no-js-hidden">
                                                        &plus;
                                                        <span class="visually-hidden">{{ 'products.product.quantity.increase' | t: product: product.title | escape }}</span>
                                                    </button>
                                                </quantity-input>
                                            {% endif %}
											<div class="product-actions add-to-cart-wrapper">
												<div class="product-actions-add-to-cart">
													

													{% if block.settings.show_add_to_cart %}
														<button
                              data-aos="zoom-in" data-aos-duration="250"
															type="submit"
															name="add"
															class="button add-cart-button button-add-to-cart {% if show_dynamic_checkout %} button-secondary{% endif %}"
															{% if product.selected_or_first_available_variant.available == false
																or product.selected_or_first_available_variant == nil
															%}
																disabled
															{% endif %}
														>
															{% if block.settings.cart_image != blank %}
																<img src="{{ block.settings.cart_image | image_url: width: 24 }}" 
																	 alt="{{ block.settings.cart_image.alt | default: 'Cart icon' }}"
																	 width="24"
																	 height="24"
																	 loading="lazy">
		
															{% endif %}
															{%- if product.selected_or_first_available_variant == nil -%}
																<span>{{ block.settings.unavailable_text | default: 'products.product.availability.unavailable' }}</span>
															{%- elsif product.selected_or_first_available_variant.available and is_pre_order -%}
																<span>{{ block.settings.pre_order_text | default: 'products.product.pre_order' }}</span>
															{%- elsif product.selected_or_first_available_variant.inventory_policy == 'continue' and product.selected_or_first_available_variant.inventory_quantity <= 0 -%}
																<span>{{ block.settings.backorder_text | default: 'products.product.backorder' }}</span>
															{%- elsif product.selected_or_first_available_variant.available -%}
																<span>{{ block.settings.add_to_cart_text | default: 'products.product.add_to_cart' }}</span>
															{%- else -%}
																<span>{{ block.settings.sold_out_text | default: 'products.product.availability.out_of_stock' }}</span>
															{%- endif -%}

															<div class="button-overlay-spinner hidden">
																<span class="spinner-xs"></span>
															</div>
														</button>
													{% endif %}
												</div>

												{% if show_dynamic_checkout %}
													<div class="product-actions-buy-now">
														{{ form | payment_button }}
													</div>
												{% endif %}
											</div>
										{% endform %}
									</product-form>
								{% endunless %}

                                {% if false and product.metafields.custom.configurable_product_type.value != blank %}
